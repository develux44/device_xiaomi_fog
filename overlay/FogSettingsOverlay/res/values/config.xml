<?xml version="1.0" encoding="utf-8"?>

<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Defines the location of the fingerprint sensor on the device
         0 = back
         1 = front
         2 = left side
         3 = right side
    -->
    <integer name="config_fingerprintSensorLocation">0</integer>
    <!-- Smartcharging -->
    <bool name="config_supportSmartCharging">true</bool>

    <!-- For Enabling Battery Heath -->
    <bool name="config_supportBatteryHealth">true</bool>

    <!-- Battery Health -->
    <string name="config_batDesCap">/sys/class/power_supply/bms/charge_full_design</string>
    <string name="config_batCurCap">/sys/class/power_supply/bms/charge_full</string>
    <string name="config_batChargeCycle">/sys/class/power_supply/bms/cycle_count</string>
    <!-- Show battery cycle count -->
    <bool name="config_showBatteryCycleCount" translatable="false">true</bool>
</resources>
